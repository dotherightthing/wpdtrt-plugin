{
    "name": "dotherightthing/wpdtrt-plugin-boilerplate",
    "description": "Boilerplate for WordPress plugin development.",
    "type": "library",
    "keywords": ["boilerplate", "framework"],
    "license": "ISC",
    "authors": [
        {
            "name": "Dan Smith",
            "email": "dev@dotherightthing.co.nz",
            "homepage": "http://dotherightthing.co.nz",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@dotherightthing.co.nz",
        "issues": "https://github.com/dotherightthing/wpdtrt-tourdates/issues"
    },
    "require": {
        "dotherightthing/wpdtrt-debug": "^1.0.0",
        "gamajo/template-loader": "^1.3.0",
        "php": ">=5.6.30"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "2.9.0",
        "phpunit/phpunit": "^5.7",
        "psy/psysh" : "~0.6",
        "wp-cli/wp-cli" : "~0.22"
    },
    "autoload": {
        "psr-4": {
            "DoTheRightThing\\WPDTRT_Plugin_Boilerplate\\r_1_4_21\\": "src"
        }
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/dotherightthing/"
        },
        {
            "packagist.org": false
        }
    ],
    "config": {
        "vendor-dir": "vendor"
    },
    "extra": [
        {
            "require-after-phpdoc": "tgmpa/tgm-plugin-activation:2.6.*"
        }
    ]
}
